{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "type": "object",
  "required": [
    "admins",
    "arcade",
    "denom",
    "max_top_score",
    "price_peer_game"
  ],
  "properties": {
    "admins": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "arcade": {
      "type": "string"
    },
    "denom": {
      "type": "string"
    },
    "max_top_score": {
      "type": "integer",
      "format": "uint8",
      "minimum": 0.0
    },
    "price_peer_game": {
      "type": "integer",
      "format": "uint128",
      "minimum": 0.0
    }
  }
}
